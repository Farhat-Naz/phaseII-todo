{
  "buildCommand": "cd frontend && pnpm build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && pnpm install",
  "framework": "nextjs",
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@4.3.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    }
  ]
}
